{
  "name": "fablo-test-network-siswa",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "Siswa",
    "tlsEnable": false,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "Siswa": {
      "mspid": "SiswaMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/siswa.maarif.ac.id/users/Admin@siswa.maarif.ac.id/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.siswa.maarif.ac.id"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/siswa.maarif.ac.id/users/Admin@siswa.maarif.ac.id/msp/signcerts/Admin@siswa.maarif.ac.id-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.siswa.maarif.ac.id": {
      "url": "grpc://peer0.siswa.maarif.ac.id:7121",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/siswa.maarif.ac.id/msp/tlscacerts/tlsca.siswa.maarif.ac.id-cert.pem"
      }
    }
  },
  "channels": {
    "ijazah": {
      "peers": {}
    },
    "sertifikat": {
      "peers": {}
    }
  }
}
